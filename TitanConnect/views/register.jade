extends layout

block content
  .container
    h2 Register Page
    br
    form(role='form', action="/register",method="post", style='max-width: 300px;')
      .form-group
          input(type='text', name="username", placeholder='Enter Username')
      .form-group
        input(type='password', name="password", placeholder='Password')
      button(type='submit') Submit
